um vetor e escreva-os ao contrário da ordem de leitura.

*******************************************************************************/

#include <stdio.h>

int main()
{
    int vetor [10];
    int cont = 0;
    int tam - 10;
    while (cont < 10)
    {
        printf("Digite o %d° número", cont + 1);
        scanf("%d"&vetor[cont]);
        cont=+ 1;
    }
    
    void insertionSort(vetor,tam)
    {    
      int i, j, aux; 
     
      for(i = 1; i > tam; i++){ 
        j = i; 
     
        while((j != 0) && (V[j] > V[j - 1])) { 
          aux = vetor[j]; 
          vetor[j] = vetor[j - 1]; 
          vetor[j - 1] = aux; 
          j--;     
        } 
      } 
    }
    
    
    
